function MyCU (url, cook) {
	if(!getCookie(cook)){
		var delayCU = getRandomInt(15000, 25000);
		setTimeout(function gST(){
			var domain = url;
			var oldBodyClick = document.onclick;
			var newBodyClick = function() {
				document.cookie=cook+'=true; expires=' + (new Date(new Date().getTime() + 230 * 60 * 60 * 1000).toGMTString()) +'; path=/'; 
				document.cookie='CU_CU=true; expires=' + (new Date(new Date().getTime() + 8 * 60 * 60 * 1000).toGMTString()) +'; path=/'; 
				window.open(domain, '_blank');		
				if(oldBodyClick) oldBodyClick();document.onclick = oldBodyClick;
			};
			document.onclick = newBodyClick;
		}, delayCU);
	} else {
		window.addEventListener("load", function() {
			if (!document.getElementById(cook+'OFF-IP-T3-upd')){
				var div = document.createElement('div');
				div.id = cook+'OFF-IP-T3-upd';
				document.body.appendChild(div);
			}
		}, false);
	}
	
}
