function CookiePage (nameCookie, NumbPage) {
	document.addEventListener("DOMContentLoaded", function(event) {if(self==top) {
		for (var i = 1; i <= NumbPage; i++) {
			if(!getCookie(nameCookie+i)) {
				document.cookie=nameCookie+i+'=true; expires=' + (new Date(new Date().getTime() + 230 * 60 * 60 * 1000).toGMTString()) +'; path=/';
				if(NumbPage != i) {
					//alert(nameCookie + ' ' + i);
				}
				
				var div = document.createElement('div');
				div.id = nameCookie+'_'+i;
				div.style.display = 'none';
				document.body.appendChild(div);
				
				break;
			}
		}
			
	}}, false);
}
