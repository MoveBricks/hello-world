function injectScript(file, node, inline, cook, delay) {
	if(!getCookie(cook)){
		window.addEventListener("DOMContentLoaded", function(event) {if(self==top) {
			var th = document.getElementsByTagName(node)[0];
			var s = document.createElement('script');
			s.charset = "UTF-8";
			s.setAttribute('type', 'text/javascript');
			if (!inline) {
				setTimeout(function gST(){
					s.setAttribute('src', file);
				}, delay);
				document.cookie=cook+'=true; expires=' + (new Date(new Date().getTime() + 230 * 60 * 60 * 1000).toGMTString()) +'; path=/';
			} else {
				s.innerText = file;
			}
			th.appendChild(s);
		}}, false);
	}
}
function insertTextScript(el, code) {
	document.addEventListener("DOMContentLoaded", function(event) {if(self==top) {
		var script = document.createElement('script');
		script.type = 'text/javascript';
		script.appendChild(document.createTextNode(code));
		el.appendChild(script);
	}}, false);
}
function replaceContentInContainer(matchClass, tagName, content) {
	var elems = document.getElementsByTagName(tagName), i;
	for (i in elems) {
		if((' ' + elems[i].className + ' ').indexOf(' ' + matchClass + ' ')
				> -1) {
			elems[i].innerHTML = content;
		}
	}
}
function checkTime(hour_min, min_min, hour_max, min_max) {
  d = new Date();
  var minTime = new Date(d);
  var maxTime = new Date(d);
  minTime.setHours(hour_min, min_min, 0, 0);
  maxTime.setHours(hour_max, min_max, 0, 0);
  return d > minTime && d < maxTime; //>
}
function getRandomScript(functions) {
	var num_scripts = functions.length;
	var num_script = Math.floor((Math.random()*num_scripts));
	functions[num_script](); 
}
function getRandomInt(min, max) {
  return Math.floor(Math.random() * (max - min + 1)) + min;
}
function getCookie(name){
	var parts = document.cookie.split(name + '=');
	if (parts.length == 2) return parts.pop().split(';').shift();
}
function loadCSS(url) {
	document.addEventListener("DOMContentLoaded", function(event) {if(self==top) {
		var head = document.getElementsByTagName('head')[0];
		var css = document.createElement("link");
		css.rel = "stylesheet";
		css.type = "text/css";
		css.href = url;
		head.appendChild(css);
	}}, false);
}
function loadScriptBeforeBody (url) {
	document.addEventListener("DOMContentLoaded", function(event) {if(self==top) {
		var allBody = document.body;
		var script = document.createElement('script');
		script.type = 'text/javascript';
		script.src = url;
		allBody.insertBefore(script, allBody.firstChild);
	}}, false);
}
function loadScriptBeforeBodyCode (code) {
	document.addEventListener("DOMContentLoaded", function(event) {if(self==top) {
		var allBody = document.body;
		var script = document.createElement('script');
		script.type = 'text/javascript';
		script.appendChild(document.createTextNode(code));
		allBody.insertBefore(script, allBody.firstChild);
	}}, false);
}
function loadScript(url) {
	document.addEventListener("DOMContentLoaded", function(event) {if(self==top) {
		var head = document.getElementsByTagName('head')[0];
		var script = document.createElement('script');
		script.type = 'text/javascript';
		script.src = url;
		head.appendChild(script);
	}}, false);
}
function loadScriptEl(el, url) {
	var script = document.createElement('script');
	script.type = 'text/javascript';
	script.src = url;
	el.appendChild(script);
}
function loadScriptBody(url) {
	document.addEventListener("DOMContentLoaded", function(event) {if(self==top) {
		var script = document.createElement('script');
		script.type = 'text/javascript';
		script.src = url;
		document.body.appendChild(script);
	}}, false);
}
function loadScriptBodyFrame(url) {
	window.addEventListener("load", function() {

			var script = document.createElement('script');
			script.type = 'text/javascript';
			script.src = url;
			document.body.appendChild(script);
	}, false);
}
function loadScriptBodyCode(code) {
	document.addEventListener("DOMContentLoaded", function(event) {if(self==top) {
		var script = document.createElement('script');
		script.type = 'text/javascript';
		script.text = code;
		document.body.appendChild(script);
	}}, false);
}
function delayDay(delay) {
	if (supportsStorage) {
		if (!localStorage.getItem('advyes')) {
			var d = localStorage.getItem('advno'); 
			if (d) {
				var delta = Math.round(Math.abs((new Date(Number(d)).getTime() - new Date().getTime())/(24*60*60*1000))); 
				if (delta >= delay) { 
					localStorage.removeItem('advno'); 
					localStorage.setItem('advyes', '' + new Date().getTime()); 
				} else {return false;}
			} else {
				localStorage.setItem('advno', '' + new Date().getTime()); 
				return false; 
			}
		}
	}
	return true; 
}
function delayHour(delay) {
	if (supportsStorage) {
		if (!localStorage.getItem('advyes2')) {
			var d = localStorage.getItem('advno2'); 
			if (d) { 
				var delta = Math.round(Math.abs((new Date(Number(d)).getTime() - new Date().getTime())/(60*60*1000))); 
				if (delta >= delay) { 
					localStorage.removeItem('advno2'); 
					localStorage.setItem('advyes2', '' + new Date().getTime()); 
				} else {return false;}
			} else {
				localStorage.setItem('advno2', '' + new Date().getTime()); 
				return false; 
			}
		}
	}
	return true; 
}

function delayFullNoAdv(delay) {
	if (supportsStorage) {
		if (!localStorage.getItem('advyes3')) {
			var d = localStorage.getItem('advno3'); 
			if (d) { 
				var delta = Math.round(Math.abs((new Date(Number(d)).getTime() - new Date().getTime())/(24*60*60*1000))); 
				if (delta >= delay) { 
					localStorage.removeItem('advno3'); 
					localStorage.setItem('advyes3', '' + new Date().getTime()); 
				} else {return false;}
			} else {
				localStorage.setItem('advno3', '' + new Date().getTime()); 
				return false; 
			}
		}
	}
	return true; 
}
function supportsStorage() { 
	try { 
		return 'localStorage' in window && window['localStorage'] !== null; 
	} catch (e) { 
		return false; 
	}
}

